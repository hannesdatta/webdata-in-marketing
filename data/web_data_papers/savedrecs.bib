@article{ WOS:000684147300006,
Author = {To, Rita Ngoc and Patrick, Vanessa M.},
Title = {{How the Eyes Connect to the Heart: The Influence of Eye Gaze Direction
   on Advertising Effectiveness}},
Journal = {{JOURNAL OF CONSUMER RESEARCH}},
Year = {{2021}},
Volume = {{48}},
